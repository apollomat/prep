class Solution(object):
    def maxProduct(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        if(len(nums) == 0):
            return 0
        if(len(nums) == 1):
            return nums[0]
        min_product = nums[0]
        max_product = nums[0]
        global_max = nums[0]
        for i in range(1, len(nums)):
            temp_max = max_product
            max_product = max(max_product * nums[i], min_product * nums[i], nums[i])
            min_product = min(temp_max * nums[i], min_product * nums[i], nums[i])
            global_max = max(max_product, global_max)
            
        return global_max
        
